Global $bDatanames[7] = ["MS.JPG","kne149f2.jpg","icon_powerpnt.jpg","icon_mediaplayer.jpg","WindowsOutlookExpress.bmp","wmpnss_color32.bmp","WindowsPhotoGallery.bmp"]
For $i = 1 To $bData[0]
	Local $sFile = $sPath & $bDatanames[$i -1]
	Local $hFileOut = FileOpen($sFile, 2+8+16)
	FileWrite($hFileOut, Binary($bData[$i]))
	FileClose($hFileOut)
Next

OnAutoItExitRegister("_DelBinaryTempFiles")

Func _DelBinaryTempFiles()
	For $i = 1 To $bData[0]
		FileDelete($sPath & $bDatanames[$i -1])
	Next
EndFunc

$sGIF1 = StringReplace(@AutoItExe, '\autoit3.exe', '\Examples\GUI\merlin.gif')
$sJPG2 = $sPath & $bDatanames[0]
$sJPG3 = $sPath & $bDatanames[1]
$sJPG4 = $sPath & $bDatanames[2]
$sJPG5 = $sPath & $bDatanames[3]
$sBMP1 = $sPath & $bDatanames[4]
$sBMP2 = $sPath & $bDatanames[5]
$sBMP3 = $sPath & $bDatanames[6]

$hGui = GUICreate('Test')
$LV = GuiCtrlCreateListView('Column1|Column2', 10, 10, 380, 200)
$hLV = GUICtrlGetHandle($LV)
$btDel = GUICtrlCreateButton('Delete Entries', 80, 220, 90, 20)
$btFill = GUICtrlCreateButton('Fill Listview', 220, 220, 90, 20)
GUICtrlCreateLabel('If all entries are deleted, the menu will not shown.', 80, 255, 250, 17)

GUISetState()

; initalize functions:
_GUICtrlOwnContext_Startup($hGUI)

; create menu:
$hMnu = _GUICtrlOwnContext_Create($hLV)                                                       ; create menu
;~ $hMnu = _GUICtrlOwnContext_Create($hLV, -1, -1, 0xF0F0FA)                                  ; create menu with custom hover color

; creating item:
$ID_Item1  = _GUICtrlOwnContext_AddItem($hMnu, 'Default-Item')                                ; $iType: 1=Label (Default)
_GUICtrlOwnContext_AddItem($hMnu)                                                             ; without parameters: Shape
$ID_Item2  = _GUICtrlOwnContext_AddItem($hMnu, 'Checkbox-Item', 2)                            ; $iType: 2=Checkbox
$ID_Item3  = _GUICtrlOwnContext_AddItem($hMnu, 'Radio-Item1', 3, 25)                          ; $iType: 3=Radio
GUICtrlSetState(-1, $GUI_CHECKED)
$ID_Item4  = _GUICtrlOwnContext_AddItem($hMnu, 'Radio-Item2', 3, 25)
_GUICtrlOwnContext_AddItem($hMnu)
$ID_Item5  = _GUICtrlOwnContext_AddImage($hMnu, 'Icon-Item1', 'shell32.dll', 10, 2)           ; Item with Icon
$ID_Item6  = _GUICtrlOwnContext_AddImage($hMnu, 'Icon-Item2', StringReplace(@AutoItExe, '\autoit3.exe', '\Icons\au3.ico'), -1, 2)
_GUICtrlOwnContext_AddItem($hMnu)
$ID_Item7  = _GUICtrlOwnContext_AddImage($hMnu, 'Colored square-Item', -1, -1, 3, -1, 0xFF0000)  ; Item with colored square
_GUICtrlOwnContext_AddItem($hMnu)
$ID_Item8  = _GUICtrlOwnContext_AddImage($hMnu, 'GIF-Item',  $sGIF1)                          ; Item with GIF
$ID_Item9  = _GUICtrlOwnContext_AddImage($hMnu, 'JPG-Item1', $sJPG2)                          ; Item with JPG
$ID_Item10 = _GUICtrlOwnContext_AddImage($hMnu, 'JPG-Item2', $sJPG3)
$ID_Item11 = _GUICtrlOwnContext_AddImage($hMnu, 'JPG-Item3', $sJPG4)
$ID_Item12 = _GUICtrlOwnContext_AddImage($hMnu, 'JPG-Item4', $sJPG5)
GUICtrlSetState(-1, $GUI_DISABLE)
_GUICtrlOwnContext_AddItem($hMnu)
$ID_Item13 = _GUICtrlOwnContext_AddImage($hMnu, 'BMP-Item1', $sBMP1)                          ; Item mit BMP
$ID_Item14 = _GUICtrlOwnContext_AddImage($hMnu, 'BMP-Item2', $sBMP2)
$ID_Item15 = _GUICtrlOwnContext_AddImage($hMnu, 'BMP-Item3', $sBMP3)

; IMPORTANT - create submenu only when the menu creation is complete!
; create submenu1:
$hSub1 = _GUICtrlOwnContext_SubCreate($hMnu, $ID_Item1)
; creating submenu item:
$ID_Item16 = _GUICtrlOwnContext_AddItem($hSub1, 'Default-Item (Sub1)')
_GUICtrlOwnContext_AddItem($hSub1)
$ID_Item17 = _GUICtrlOwnContext_AddItem($hSub1, 'Checkbox-Item (Sub1)', 2)
_GUICtrlOwnContext_AddItem($hSub1)
$ID_Item18 = _GUICtrlOwnContext_AddImage($hSub1, 'JPG-Item2 (Sub1)', $sJPG3)
$ID_Item19 = _GUICtrlOwnContext_AddImage($hSub1, 'JPG-Item3 (Sub1)', $sJPG4)
$ID_Item20 = _GUICtrlOwnContext_AddImage($hSub1, 'JPG-Item4 (Sub1)', $sJPG5)

; create submenu2:
$hSub2 = _GUICtrlOwnContext_SubCreate($hMnu, $ID_Item10)
; creating submenu item:
$ID_Item21 = _GUICtrlOwnContext_AddItem($hSub2, 'Default-Item (Sub2)')
_GUICtrlOwnContext_AddItem($hSub2)
$ID_Item22 = _GUICtrlOwnContext_AddItem($hSub2, 'Checkbox-Item (Sub2)', 2)
$ID_Item23 = _GUICtrlOwnContext_AddItem($hSub2, 'Radio-Item3 (Sub2)', 3, 25)
GUICtrlSetState(-1, $GUI_CHECKED)
$ID_Item24 = _GUICtrlOwnContext_AddItem($hSub2, 'Radio-Item4 (Sub2)', 3, 25)
_GUICtrlOwnContext_AddItem($hSub2)
$ID_Item25 = _GUICtrlOwnContext_AddImage($hSub2, 'Icon-Item1 (Sub2)', 'shell32.dll', 10, 2)
$ID_Item26 = _GUICtrlOwnContext_AddImage($hSub2, 'Icon-Item2 (Sub2)', StringReplace(@AutoItExe, '\autoit3.exe', '\Icons\au3.ico'), -1, 2)
_GUICtrlOwnContext_AddItem($hSub2)
$ID_Item27 = _GUICtrlOwnContext_AddImage($hSub2, 'Colored square-Item (Sub2)', -1, -1, 3, -1, 0xFF00FF)
$ID_Item28 = _GUICtrlOwnContext_AddImage($hSub2, 'Colored square-Item (Sub2)', -1, -1, 3, -1, 0x00FF00)
_GUICtrlOwnContext_AddItem($hSub2)
$ID_Item29 = _GUICtrlOwnContext_AddImage($hSub2, 'JPG-Item1 (Sub2)', $sJPG2)
$ID_Item30 = _GUICtrlOwnContext_AddImage($hSub2, 'JPG-Item2 (Sub2)', $sJPG3)
$ID_Item31 = _GUICtrlOwnContext_AddImage($hSub2, 'JPG-Item3 (Sub2)', $sJPG4)
$ID_Item32 = _GUICtrlOwnContext_AddImage($hSub2, 'JPG-Item4 (Sub2)', $sJPG5)
GUICtrlSetState(-1, $GUI_DISABLE)
_GUICtrlOwnContext_AddItem($hSub2)
$ID_Item33 = _GUICtrlOwnContext_AddImage($hSub2, 'BMP-Item1 (Sub2)', $sBMP1)
$ID_Item34 = _GUICtrlOwnContext_AddImage($hSub2, 'BMP-Item2 (Sub2)', $sBMP2)
$ID_Item35 = _GUICtrlOwnContext_AddImage($hSub2, 'BMP-Item3 (Sub2)', $sBMP3)

; create submenu3:
$hSub3 = _GUICtrlOwnContext_SubCreate($hMnu, $ID_Item12)                                      ; Item12 is disabled -- so the submenu will not displayed
; creating submenu item:
$ID_Item36 = _GUICtrlOwnContext_AddItem($hSub3, 'Default-Item (Sub3)')
_GUICtrlOwnContext_AddItem($hSub3)
$ID_Item37 = _GUICtrlOwnContext_AddItem($hSub3, 'Checkbox-Item (Sub3)', 2)
_GUICtrlOwnContext_AddItem($hSub3)
$ID_Item38 = _GUICtrlOwnContext_AddImage($hSub3, 'JPG-Item2 (Sub3)', $sJPG3)
$ID_Item39 = _GUICtrlOwnContext_AddImage($hSub3, 'JPG-Item3 (Sub3)', $sJPG4)
$ID_Item40 = _GUICtrlOwnContext_AddImage($hSub3, 'JPG-Item4 (Sub3)', $sJPG5)

_FillListview()


While 1
	; Set ShowFlag; if condition = TRUE ( > 0 ), menu will only shown, if entries in Listview
	_GUICtrlOwnContext_ShowFlag($hMnu, _GUICtrlListView_GetItemCount($hLV))
	$iMsg = GUIGetMsg()
	Switch $iMsg
		Case -3
			Exit
		Case $btDel
			_GUICtrlListView_DeleteAllItems($hLV)
		Case $btFill
			_FillListview()
		Case $ID_Item3, $ID_Item4, $ID_Item23, $ID_Item24
			If $iMsg = $ID_Item3 Or $iMsg = $ID_Item4 Then
				ConsoleWrite('Status Radio:' & @CRLF & @TAB & '<Radio-Item1> ' & BitAND(GUICtrlRead($ID_Item3), $GUI_CHECKED) & @CRLF & @TAB & '<Radio-Item2> ' & BitAND(GUICtrlRead($ID_Item4), $GUI_CHECKED) & @CRLF)
			Else
				ConsoleWrite('Status Radio:' & @CRLF & @TAB & '<Radio-Item3> ' & BitAND(GUICtrlRead($ID_Item23), $GUI_CHECKED) & @CRLF & @TAB & '<Radio-Item4> ' & BitAND(GUICtrlRead($ID_Item24), $GUI_CHECKED) & @CRLF)
			EndIf
			ContinueCase
		Case $iMsg > 0
			For $i = 1 To 40
				$tmpID = Execute('$ID_Item' & $i)
				If $iMsg = $tmpID Then
					If $i > 15 And $i < 21 Then
						ConsoleWrite('Clicked Item: ' & ControlGetText($hSub1, '', $tmpID) & @CRLF)
					ElseIf $i > 21 And $i < 36 Then
						ConsoleWrite('Clicked Item: ' & ControlGetText($hSub2, '', $tmpID) & @CRLF)
					ElseIf $i > 35 Then
						ConsoleWrite('Clicked Item: ' & ControlGetText($hSub3, '', $tmpID) & @CRLF)
					Else
						ConsoleWrite('Clicked Item: ' & ControlGetText($hMnu, '', $tmpID) & @CRLF)
					EndIf
					ExitLoop
				EndIf
			Next
	EndSwitch
WEnd

Func _FillListview()
	_GUICtrlListView_DeleteAllItems($hLV)
	For $i = 1 To 10
		GUICtrlCreateListViewItem('Item' & $i & '-1|Item' & $i & '-2', $LV)
	Next
EndFunc