#NoTrayIcon
#include <ButtonConstants.au3>
#include <GUIConstantsEx.au3>
#include <WindowsConstants.au3>
#include <StaticConstants.au3>
#include <String.au3>
$font="Arial"
global $form2, $lavel1, $label2, $input, $Form1, $Form4, $form5, $i = 0,$input4,$label5, $label6, $input3, $input4,$button_ok, $input2, $label4, $locked, $forgotpassword
Opt("TrayMenuMode", 1)
Opt("TrayOnEventMode",1)
Opt("GUIOnEventMode", 1)
$InputPass = "snd"
$InputLevel = 1
If RegRead("HKEY_LOCAL_MACHINE\SOFTWARE\snd\desktoplocker\", "Encripted Password")="" then
    $password = ""
Else
    $string1 = RegRead("HKEY_LOCAL_MACHINE\SOFTWARE\snd\desktoplocker\", "Encripted Password")
    $password = _StringEncrypt(0,$string1,$InputPass,$InputLevel)
EndIf
If Not FileExists(@WindowsDir & "\Lock Dektop Main.jpg") Then
_WriteNewfile_1ToDir(@WindowsDir & "\Lock Dektop Main.jpg")
EndIf
$locked = 0
$Form1 = GUICreate("Desktop is locked", @DesktopWidth, @DesktopHeight, 0, 0, $ws_popup)
GuiSetIcon(@SystemDir & "\SHELL32.dll", 48)
GUISetState(@SW_HIDE,$Form1)
GUISetBkColor(0x000000)
GUICtrlCreatePic("C:\WINDOWS\Web\Wallpaper\Windows XP.jpg",0,0,@DesktopWidth,@DesktopHeight)
WinSetOnTop("Desktop is locked", "", 1)
TraySetIcon(@SystemDir & "\SHELL32.dll",44)
$lock = TrayCreateItem("Lock desktop")
TrayItemSetState(-1, 512)
$changepassword = TrayCreateItem("Change password")
$forgotpassword = TrayCreateItem("Forgot password")
$startup = TrayCreateItem("Start with Windows")
TrayCreateItem("")
$ExitItem = TrayCreateItem("Exit")
TraySetClick(16)

TrayItemSetOnEvent($lock, "zakljucaj")
TrayItemSetOnEvent($changepassword, "menjajpass")
TrayItemSetOnEvent($forgotpassword, "zaboravljenpas")
TrayItemSetOnEvent($startup, "startup")
TrayItemSetOnEvent($ExitItem, "izlaz")

If RegRead("HKEY_LOCAL_MACHINE\SOFTWARE\snd\desktoplocker\", "Forgot Password")="1" then
    TrayItemSetState($forgotpassword,$GUI_CHECKED)
	$fpw = "1"
Else
	$fpw = "0"
EndIf

If RegRead("HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Run", "Desktop Locker") <> "" Then
    TrayItemSetState($startup,$GUI_CHECKED)
	$suw = "1"
Else
	$suw = "0"
EndIf

TraySetState()
TraySetToolTip("Desktop Locker")
TrayTip("Desktop Locker", "Click here to lock desktop," &@CRLF& "change password or exit.", 5,1)

AdlibEnable ("adlib1", 250)

While 1
If UBound(ProcessList(@ScriptName)) > 2 Then
    Exit
EndIf
If $locked = "1" Then
;~ 	If BitAND(WinGetState("Desktop is locked"), @SW_SHOWNOACTIVATE ) = @SW_SHOWNOACTIVATE Then GUISetState(@SW_RESTORE, $Form2)
	If ProcessExists("taskmgr.exe") Then
		GUICtrlSetData($label2, "Task Manager couldn't help you :)")
		Sleep(1000)
		ProcessClose("taskmgr.exe")
	EndIf
EndIf
WEnd

func menjajpass ()
	If $locked = 0 Then
    $i = 1
    $Form4 = Guicreate("Change password", 300, 205, -1, -1,$WS_POPUP, $WS_EX_DLGMODALFRAME, $Form1)
    GUISetFont (9, 800, Default, $font)
    $label4 = GUICtrlCreateLabel("Please type in your old password:", 50, 10, 260, 20)
    $input2 = GUICtrlCreateInput("", 20, 35, 260, 20, 0x21)
    $label5 = GUICtrlCreateLabel("Please type in your new password:", 50, 65, 260, 20)
    GUICtrlSetState(-1, $GUI_DISABLE)
    $input3 = GUICtrlCreateInput("", 20, 90, 260, 20, 0x21)
    GUICtrlSetState(-1, $GUI_DISABLE)
    $label6 = GUICtrlCreateLabel("Please confirm your new password:", 50, 120, 260, 20)
    GUICtrlSetState(-1, $GUI_DISABLE)
    $input4 = GUICtrlCreateInput("", 20, 145, 260, 20, 0x21)
    GUICtrlSetState(-1, $GUI_DISABLE)
    $button_ok = GUICtrlCreateButton("OK", 65, 175, 75, 23)
    GUICtrlSetState(-1, $GUI_DISABLE)
    $button_cancel = GUICtrlCreateButton("Cancel", 160, 175, 75, 23)
    GUICtrlSetOnEvent($button_ok, "potvrdi")
    GUICtrlSetOnEvent($button_cancel, "prekini")
    GUISetState(@SW_SHOW)
Else
	GUICtrlSetData($label2, "First unlock Desktop, then go to change password!")
	EndIf
EndFunc

Func zaboravljenpas()
If $locked = "0" Then
	If $fpw = "0" Then
		$fpw = "1"
		TrayTip("Desktop Locker", "When type in ""forgot password"""&@CRLF&"will be entered password!", 5, 1)
		TrayItemSetState($forgotpassword,$GUI_CHECKED)
		RegWrite("HKEY_LOCAL_MACHINE\SOFTWARE\snd\desktoplocker\", "Forgot Password", "REG_SZ", "1")
	Else
		$fpw = "0"
		TrayItemSetState($forgotpassword,$GUI_UNCHECKED)
		RegWrite("HKEY_LOCAL_MACHINE\SOFTWARE\snd\desktoplocker\", "Forgot Password", "REG_SZ", "0")
	EndIf
Else
	GUICtrlSetData($label2, "First unlock Desktop, then go to this option!")
EndIf
EndFunc

Func startup()
	If $suw = "0" Then
	$suw = "1"
	TrayItemSetState($startup,$GUI_CHECKED)
	RegWrite('HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Run', 'Desktop Locker', 'REG_SZ', @ScriptFullPath)
Else
	$suw = "0"
	TrayItemSetState($startup,$GUI_UNCHECKED)
	RegDelete("HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Run", "Desktop Locker")
	EndIf
EndFunc

func prekini()
    GUIDelete($Form4)
    $i = 0
EndFunc

Func potvrdi ()
    $password = guictrlread($input4)
    $string2 = _StringEncrypt(1,$password,$InputPass,$InputLevel)
    RegWrite("HKEY_LOCAL_MACHINE\SOFTWARE\snd\desktoplocker\", "Encripted Password", "REG_SZ", $string2)
    GUIDelete($Form4)
    $i = 0
EndFunc

func adlib1()
    if $i = 1 Then
        if GUICtrlRead($input2) = $password Then call("jebiga")
    EndIf
    if $i = 2 Then
        if guictrlread($input3) = guictrlread($input4) Then
            GUICtrlSetState($button_ok, $GUI_ENABLE)
        Else
            GUICtrlSetState($button_ok, $GUI_DISABLE)
        EndIf
    EndIf
EndFunc

func jebiga()
    GUICtrlSetState($label4, $GUI_DISABLE)
    GUICtrlSetState($input2, $GUI_DISABLE)
    GUICtrlSetState($label5, $GUI_ENABLE)
    GUICtrlSetState($label6, $GUI_ENABLE)
    GUICtrlSetState($input3, $GUI_ENABLE)
    GUICtrlSetState($input4, $GUI_ENABLE)
    ControlFocus("Change password", "", $input3)
    $i = 2
EndFunc

func zakljucaj ()
	If $locked = 0 Then
	WinSetTrans("","start",0)
	$locked = 1
	GUISetState(@SW_SHOW,$Form1)
	RegWrite("HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Policies\System", "DisableTaskMgr", "REG_DWORD", "1")
    hotKeySet("{ENTER}", "Provera")    
    $form2 = Guicreate("Type security password", 306, 156, @DesktopWidth/2-153 , @DesktopHeight/2-111, $WS_POPUP,$WS_EX_MDICHILD,$Form1)
	GUICtrlCreatePic(@WindowsDir & "\Lock Dektop Main.jpg",0,0, 0,0)
	GUICtrlSetState(-1,$GUI_DISABLE)
    GUISetFont (9, 800, Default, $font)
    $label1 = GUICtrlCreateLabel("Please type in your password:", 23, 25, 260, 20,$ss_center)
    GUISetFont (9, 800, Default, $font)
	GUICtrlSetBkColor(-1,$GUI_BKCOLOR_TRANSPARENT )
    $label2 = GUICtrlCreateLabel("", 23, 95, 260, 50,$ss_center)
    GUICtrlSetColor(-1, 0xff0000)
	GUICtrlSetBkColor(-1,$GUI_BKCOLOR_TRANSPARENT )
    $input = GUICtrlCreateInput("", 23, 60, 260, 20, 0x21)
    GUISetState(@SW_SHOW)
    WinSetOnTop("Type security password", "", 1)
	WinMinimizeAll()
Else
	GUICtrlSetData($label2, "Welcome back!")
	WinSetTrans("Desktop is locked", "", 200)
	GUISetState(@SW_SHOW,$form2)
	EndIf
EndFunc

func Izlaz ()
	If $locked = 0 Then
	RegWrite("HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Policies\System", "DisableTaskMgr", "REG_DWORD", "0")
    Exit
Else
	GUICtrlSetData($label2, "First unlock Desktop, then go to Exit :)")
	EndIf
EndFunc

func Provera()
	$pw = guictrlread($input)
	GUICtrlSetData($input, "")
    if $pw = $password Then
		$locked = 0
        call("otkljucano")
	ElseIf $pw = "Turn Off" Then
		GUICtrlSetData($label2, "Bye bye!")
		Sleep(1000)
		Shutdown(1)
	ElseIf $pw = "forgot password" Then
		If $fpw = "1" Then
		GUICtrlSetColor($label2, 0x698b22)
		GUICtrlSetData($label2, "Your password is typed in! You may now hit enter!")
		GUICtrlSetData($input, $password)
	Else
		GUICtrlSetColor($label2, 0xff0000)
		GUICtrlSetData($label2, "This option is off!")
		EndIf
	Else
		GUICtrlSetColor($label2, 0xff0000)
        GUICtrlSetData($label2, "Wrong password, please try again!")
    EndIf
EndFunc

func otkljucano()
	$locked = 0
	WinSetTrans("","start",250)
    hotKeySet("{ENTER}")
	RegWrite("HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Policies\System", "DisableTaskMgr", "REG_DWORD", "0")
    GUISetState(@SW_HIDE,$Form1)
    GUIDelete($form2)
    $form3 = Guicreate("Success", 300, 50, @DesktopWidth/2-153, @DesktopHeight/2-75, $WS_POPUP,$WS_EX_MDICHILD,$Form1)
	GUICtrlCreatePic(@WindowsDir & "\Lock Dektop Main.jpg",0,0, 300,50)
    GUICtrlSetBkColor($form3, 0xf0fff0)
    GUISetFont (10, 800, Default, $font)
    $label3 = GUICtrlCreateLabel("Succes! You have unlocked your desktop", 20, 15, 290, 20)
    GUICtrlSetColor(-1, 0x698b22)
	GUICtrlSetBkColor(-1,$GUI_BKCOLOR_TRANSPARENT )
    GUISetState(@SW_SHOW)
    Sleep(2000)
    GUIDelete($form3)
EndFunc

Func oK()
	GUIDelete($form5)
EndFunc

