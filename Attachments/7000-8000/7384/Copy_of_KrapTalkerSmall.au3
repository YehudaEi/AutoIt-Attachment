		$Ran = Random(1,149,1)
	GUICtrlSetData($TalkBox,$Str[$Ran])

	Case $msg= $Prev
		$n = $n-1
		If $n < 0 then
		$n=0
		Endif
	GUICtrlSetData($TalkBox,$Str[$n])

	Case $msg= $Next
		$n = $n+1
		If $n >149 then
		$n=149
		Endif
	GUICtrlSetData($TalkBox,$Str[$n])
;------------------------------

	EndSelect	
WEnd


Func _SetSel($Sel)
		ControlFocus($Title,"",$Model)
	ControlCommand($Title, "", $Model, "SetCurrentSelection", $Sel)
EndFunc


func _saytext($text)
		ControlFocus ($title,"",$Speechtext)
	ControlSetText ($title,"",$Speechtext,"")
	ControlSend ($title,"",$Speechtext,$text)
		ControlClick($Title, "", $Speak)
EndFunc

