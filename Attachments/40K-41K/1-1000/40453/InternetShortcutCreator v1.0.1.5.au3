	$TestPath = _PathSplit ( $sPath, $szDrive, $szDir, $szFName, $szExt )
	Return $szDrive & $szDir & $szFName & $sStringToInsert & $szExt
EndFunc  ;==> _FilePathInsertBetweenNameAndExt ()

