			If StringLeft ( $aCrackedUrl[6], 1 ) <> '/' Then $aCrackedUrl[6] = '/' & $aCrackedUrl[6]
			$sRet = $aCrackedUrl[0] & '://' & $aCrackedUrl[2] & $aCrackedUrl[6] & '/favicon.ico'
			$sFileType = _WinHttpGetFileType ( $sRet )
			If Not @error And _FileIconUrlIsTypeValid ( $sFileType ) Then Return SetError ( 0, 0, $sRet )
