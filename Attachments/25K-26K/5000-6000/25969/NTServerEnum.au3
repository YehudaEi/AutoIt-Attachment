#include <MemoryDll.au3>

Const $SV_TYPE_DOMAIN_ENUM = 0x80000000
Dim $DllBinary = ''
	$DllBinary &= ''
Dim $aRet, $Module

MemoryDllInit()
$Module = MemoryDllOpen($DllBinary)

If $Module <> 0 Then
	$aRet = MemoryDllCall($Module, 'wstr', 'NTServerEnum', "dword", $SV_TYPE_DOMAIN_ENUM)
	MsgBox(0, 'Result 1', $aRet[0])
	MemoryDllClose($Module)
EndIf